    // Token color customization for YAML wildcard scope injected by the local extension
    // If greyed out, this is likely being overshadowed by the .code-workspace settings json file. 
    // To modify that: Ctrl+Shift+P > Preferences: Open Workspace Settings (JSON)
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.other.wildcard.renamer.yaml",
                "settings": {
                    "foreground": "#2cc786",
                    "fontStyle": "bold"
                }
            }
        ]
    }
